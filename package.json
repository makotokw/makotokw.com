{
  "name": "makotokw.com",
  "version": "4.2.0",
  "description": "makotokw.com",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "eleventy": "npx eleventy",
    "eleventy-debug": "cross-env DEBUG=* npx eleventy",
    "eleventy-debug-watch": "cross-env DEBUG=* npx eleventy --watch",
    "dev": "npm-run-all clean:project build:data dev:server",
    "dev:webpack": "cross-env NODE_ENV=development npx webpack --env=dev --progress --profile --colors",
    "dev:server": "npm-run-all --parallel dev:webpack-server dev:eleventy",
    "dev:webpack-server": "cross-env NODE_ENV=development npx webpack-dev-server --env=dev",
    "dev:eleventy": "cross-env NODE_ENV=development npx eleventy --watch",
    "build": "npm-run-all clean:project build:all",
    "build:all": "npm-run-all build:data build:webpack build:eleventy",
    "build:data": "node ./build/scripts/fixtures-to-json.js && node ./build/scripts/feeds-to-json.js && node ./build/scripts/fixture-portfolio-to-template-data.js",
    "build:webpack": "cross-env NODE_ENV=production npx webpack --env=prod --progress --profile --colors",
    "build:eleventy": "cross-env NODE_ENV=production npx eleventy",
    "prod:server": "npm-run-all build prod:eleventy-server",
    "prod:eleventy-server": "cross-env NODE_ENV=production npx eleventy --serve",
    "clean:project": "node ./build/scripts/clean.js",
    "lint": "eslint --ext .js,.vue src *.js",
    "deploy": "node ./build/scripts/deploy.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.14.0",
    "axios": "^0.20.0",
    "axios-jsonp": "^1.0.4",
    "bootstrap": "^4.5.2",
    "core-js": "^3.6.5",
    "filenamify-url": "^2.1.1",
    "github-repo-widget.js": "^1.0.2",
    "headroom.js": "^0.11.0",
    "jquery": "^3.5.1",
    "loglevel": "^1.7.0",
    "moment": "^2.28.0",
    "popper.js": "^1.16.1",
    "prism-themes": "^1.4.0",
    "rss-parser": "^3.9.0",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@vue/babel-preset-app": "^4.5.6",
    "autoprefixer": "^10.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.12",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-vue": "^6.2.2",
    "expose-loader": "^1.0.0",
    "file-loader": "^6.1.0",
    "glob": "^7.1.6",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.11.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.0.5",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^4.0.2",
    "postcss-scss": "^3.0.0",
    "rimraf": "^3.0.2",
    "rsync": "^0.6.1",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.2",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.4",
    "yaml": "^1.10.0",
    "yaml-loader": "^0.6.0"
  }
}
