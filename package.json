{
  "name": "makotokw.com",
  "version": "4.2.0",
  "description": "makotokw.com",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "eleventy": "npx eleventy",
    "eleventy-debug": "cross-env DEBUG=* npx eleventy",
    "eleventy-debug-watch": "cross-env DEBUG=* npx eleventy --watch",
    "dev": "npm-run-all clean:project build:data dev:server",
    "dev:webpack": "cross-env NODE_ENV=development npx webpack --env=dev --progress --profile --colors",
    "dev:server": "npm-run-all --parallel dev:webpack-server dev:eleventy",
    "dev:webpack-server": "cross-env NODE_ENV=development npx webpack-dev-server --env=dev",
    "dev:eleventy": "cross-env NODE_ENV=development npx eleventy --watch",
    "build": "npm-run-all clean:project build:all",
    "build:all": "npm-run-all build:data build:webpack build:eleventy",
    "build:data": "node ./build/scripts/fixtures-to-json.js && node ./build/scripts/feeds-to-json.js && node ./build/scripts/fixture-portfolio-to-template-data.js",
    "build:webpack": "cross-env NODE_ENV=production npx webpack --env=prod --progress --profile --colors",
    "build:eleventy": "cross-env NODE_ENV=production npx eleventy",
    "prod:server": "npm-run-all build prod:eleventy-server",
    "prod:eleventy-server": "cross-env NODE_ENV=production npx eleventy --serve",
    "clean:project": "node ./build/scripts/clean.js",
    "lint": "eslint --ext .js,.vue src *.js",
    "deploy": "node ./build/scripts/deploy.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-pro": "^5.12.0",
    "axios": "^0.19.0",
    "axios-jsonp": "^1.0.4",
    "bootstrap": "^4.4.1",
    "core-js": "^3.6.1",
    "filenamify-url": "^2.1.1",
    "github-repo-widget.js": "^1.0.2",
    "headroom.js": "^0.10.3",
    "jquery": "^3.5.0",
    "loglevel": "^1.6.6",
    "moment": "^2.24.0",
    "popper.js": "^1.16.0",
    "prism-themes": "^1.3.0",
    "rss-parser": "^3.7.3",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.9.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.3",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@vue/babel-preset-app": "^4.1.1",
    "autoprefixer": "^9.7.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.0",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-import-resolver-webpack": "^0.12.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-vue": "^6.1.0",
    "expose-loader": "^0.7.5",
    "file-loader": "^5.0.2",
    "glob": "^7.1.6",
    "js-yaml": "^3.13.1",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^2.0.0",
    "rimraf": "^3.0.0",
    "rsync": "^0.6.1",
    "sass-loader": "^8.0.0",
    "vue-loader": "^15.8.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.4",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2",
    "yaml-loader": "^0.5.0"
  }
}
