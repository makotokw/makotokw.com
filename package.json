{
  "name": "makotokw.com",
  "version": "4.0.0",
  "description": "makotokw.com",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "eleventy": "npx eleventy",
    "eleventy-debug": "cross-env DEBUG=* npx eleventy",
    "eleventy-debug-watch": "cross-env DEBUG=* npx eleventy --watch",
    "dev": "npm-run-all clean:project build:data dev:server",
    "dev:webpack": "cross-env NODE_ENV=development npx webpack --env=dev --progress --profile --colors",
    "dev:server": "npm-run-all --parallel dev:webpack-server dev:eleventy",
    "dev:webpack-server": "cross-env NODE_ENV=development npx webpack-dev-server --env=dev",
    "dev:eleventy": "cross-env NODE_ENV=development npx eleventy --watch",
    "build": "npm-run-all clean:project build:all",
    "build:all": "npm-run-all build:data build:webpack build:eleventy",
    "build:data": "node ./build/scripts/fixtures-to-json.js && node ./build/scripts/feeds-to-json.js",
    "build:webpack": "cross-env NODE_ENV=production npx webpack --env=prod --progress --profile --colors",
    "build:eleventy": "cross-env NODE_ENV=production npx eleventy",
    "prod:server": "npm-run-all build prod:eleventy-server",
    "prod:eleventy-server": "cross-env NODE_ENV=production npx eleventy --serve",
    "clean:project": "node ./build/scripts/clean.js",
    "lint": "eslint --ext .js,.vue .",
    "deploy": "node ./build/scripts/deploy.js"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "axios-jsonp": "^1.0.2",
    "bootstrap-material-design": "^0.5.10",
    "bootstrap-sass": "^3.4.1",
    "core-js": "^2.6.9",
    "filenamify-url": "^2.1.0",
    "font-awesome": "^4.7.0",
    "github-repo-widget.js": "^1.0.2",
    "headroom.js": "^0.9.4",
    "jquery": "^1.9.1",
    "loglevel": "^1.6.3",
    "moment": "^2.24.0",
    "rss-parser": "^3.7.2",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.8.3",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@vue/babel-preset-app": "^3.10.0",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "dotenv": "^2.0.0",
    "eslint": "^6.2.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-vue": "^5.2.3",
    "expose-loader": "^0.7.5",
    "file-loader": "^3.0.1",
    "glob": "^7.1.4",
    "js-yaml": "^3.13.1",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^2.0.0",
    "rimraf": "^2.7.1",
    "rsync": "^0.6.1",
    "run-sequence": "^1.2.2",
    "sass-loader": "^7.2.0",
    "vue-loader": "^15.7.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.39.2",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.1",
    "yaml-loader": "^0.5.0"
  }
}
