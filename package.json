{
  "name": "makotokw.com",
  "version": "3.0.0",
  "description": "makotokw.com",
  "private": true,
  "license": "MIT",
  "scripts": {
    "bower:install": "bower install",
    "eleventy": "npx eleventy",
    "eleventy-debug": "cross-env DEBUG=* npx eleventy",
    "eleventy-debug-watch": "cross-env DEBUG=* npx eleventy --watch",
    "dev": "npm-run-all clean:project dev:gulp dev:webpack dev:server",
    "dev:gulp": "cross-env NODE_ENV=development npx gulp build",
    "dev:webpack": "cross-env NODE_ENV=development npx webpack --env=dev --progress --profile --colors",
    "dev:server": "npm-run-all --parallel dev:webpack-server dev:eleventy",
    "dev:webpack-server": "cross-env NODE_ENV=development npx webpack-dev-server --env=dev",
    "dev:eleventy": "cross-env NODE_ENV=development npx eleventy --watch",
    "build": "npm-run-all clean:project build:all",
    "build:all": "npm-run-all build:webpack build:eleventy",
    "build:webpack": "cross-env NODE_ENV=production npx webpack --env=prod --progress --profile --colors",
    "build:eleventy": "cross-env NODE_ENV=production npx eleventy",
    "clean:project": "node ./build/scripts/clean.js"
  },
  "dependencies": {},
  "devDependencies": {
    "@11ty/eleventy": "^0.8.2",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "autoprefixer": "^9.4.7",
    "babel-loader": "^8.0.5",
    "browser-sync": "^2.26.3",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.10",
    "del": "^2.2.0",
    "dotenv": "^2.0.0",
    "file-loader": "^3.0.1",
    "glob": "^7.1.3",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-if": "^2.0.0",
    "gulp-jshint": "^2.0.0",
    "gulp-load-plugins": "^1.2.0",
    "gulp-rsync": "0.0.6",
    "gulp-shell": "^0.5.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-template-compile": "^1.2.2",
    "gulp-util": "^3.0.0",
    "gulp-yaml": "^1.0.1",
    "jshint": "^2.9.0",
    "jshint-stylish": "^2.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^2.0.0",
    "rimraf": "^2.6.3",
    "run-sequence": "^1.2.2",
    "sass-loader": "^7.1.0",
    "webpack": "^4.30.0",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  }
}
