{
  "name": "makotokw.com",
  "version": "1.0.0",
  "dependencies": {
    "jquery": "~1.9.0",
    "jquery.easing": "~1.3.0",
    "underscore": "~1.5.0",
    "underscore.string": "~2.3.3",
    "backbone": "~1.1.2",
    "modernizr": "~2.8.3",
    "bootstrap-sass": "~3.3.0",
    "font-awesome": "~4.2",
    "isotope": "~2.0.0",
    "queryloader2": "~2.9.0",
    "bootstrap-material-design": "^0.5.9"
  },
  "overrides": {
    "jquery": {
      "main": [
        "jquery.js"
      ]
    },
    "underscore.string": {
      "main": [
        "lib/underscore.string.js"
      ]
    },
    "modernizr": {
      "main": [
        "modernizr.js"
      ]
    },
    "bootstrap-sass": {
      "main": [
        "assets/javascripts/bootstrap.js",
        "assets/fonts/bootstrap/*"
      ],
      "normalize": {
        "fonts": [
          "glyphicons-*"
        ]
      }
    },
    "font-awesome": {
      "normalize": {
        "fonts": [
          "*.otf",
          "*-webfont.*"
        ]
      }
    },
    "isotope": {
      "main": [
        "dist/isotope.pkgd.js"
      ]
    },
    "queryloader2": {
      "main": [
        "jquery.queryloader2.js"
      ]
    }
  }
}
