{
  "name": "makotokw.com",
  "version": "1.0.0",
  "dependencies": {
    "jquery": "~1.9.0",
    "jquery.easing": "~1.3.0",
    "underscore": "~1.5.0",
    "underscore.string": "~2.3.3",
    "backbone": "~1.1.2",
    "modernizr": "~2.8.3",
    "bootstrap-sass": "~3.3.0",
    "font-awesome": "~4.2",
    "isotope": "~2.0.0",
    "queryloader2": "~2.9.0"
  },
  "overrides": {
    "jquery": {
      "main": [
        "jquery.js"
      ]
    },
    "underscore.string": {
      "main": [
        "lib/underscore.string.js"
      ]
    },
    "modernizr": {
      "main": [
        "modernizr.js"
      ]
    },
    "bootstrap-sass": {
      "main": [
        "assets/javascripts/bootstrap.js",
        "assets/fonts/bootstrap/*"
      ],
      "normalize": {
        "fonts": [
          "glyphicons-*"
        ]
      }
    },
    "font-awesome": {
      "normalize": {
        "fonts": [
          "*.otf",
          "*-webfont.*"
        ]
      }
    },
    "isotope": {
      "main": [
        "dist/isotope.pkgd.js"
      ]
    },
    "queryloader2": {
      "main": [
        "jquery.queryloader2.js"
      ]
    }
  },
  "exportsOverride": {
    "jquery": {
      "javascripts": [
        "jquery*.js",
        "jquery*.map"
      ]
    },
    "jquery.easing": {
      "javascripts": [
        "js/jquery.easing.js"
      ]
    },
    "jquery-ui": {
      "javascripts": "ui/jquery-ui.js"
    },
    "underscore": {
      "javascripts": [
        "underscore*.js",
        "underscore*.map"
      ]
    },
    "underscore.string": {
      "javascripts": [
        "lib/underscore.string.js"
      ]
    },
    "backbone": {
      "javascripts": [
        "backbone*.js"
      ]
    },
    "modernizr": {
      "javascripts": [
        "modernizr.js"
      ]
    },
    "bootstrap-sass-official": {
      "javascripts": "assets/javascripts/*.js",
      "javascripts/bootstrap": "assets/javascripts/bootstrap/*.js",
      "stylesheets": "assets/stylesheets/*.scss",
      "stylesheets/bootstrap": "assets/stylesheets/bootstrap/*.scss",
      "stylesheets/bootstrap/mixins": "assets/stylesheets/bootstrap/mixins/*.scss",
      "fonts": "assets/fonts/bootstrap/*.*"
    },
    "font-awesome": {
      "stylesheets": "scss/*.scss",
      "fonts": "fonts/*.*"
    },
    "queryloader2": {
      "javascripts": [
        "jquery.queryloader2.js"
      ]
    },
    "isotope": {
      "javascripts": [
        "dist/isotope.pkgd.js"
      ]
    }
  }
}
