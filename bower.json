{
  "name": "makotokw.com",
  "version": "1.0.0",
  "dependencies": {
    "jquery": "~1.9.0",
    "jquery.easing": "~1.3.0",
    "underscore": "~1.8.3",
    "underscore.string": "~2.4.0",
    "backbone": "~1.3.3",
    "modernizr": "~2.8.3",
    "bootstrap-sass": "~3.3.0",
    "font-awesome": "~4.6",
    "isotope": "~2.0.0",
    "queryloader2": "~2.9.0",
    "bootstrap-material-design": "~0.5.10"
  },
  "resolutions": {
    "jquery": "1.9.1"
  },
  "overrides_comment": "for main-bower-file: https://github.com/ck86/main-bower-files#overrides-options",
  "overrides": {
    "jquery": {
      "main": [
        "jquery.js"
      ]
    },
    "underscore.string": {
      "main": [
        "lib/underscore.string.js"
      ]
    },
    "modernizr": {
      "main": [
        "modernizr.js"
      ]
    },
    "bootstrap-sass": {
      "main": [
        "assets/javascripts/bootstrap.js",
        "assets/fonts/bootstrap/*"
      ],
      "normalize": {
        "fonts": [
          "glyphicons-*"
        ]
      }
    },
    "font-awesome": {
      "comment": "directly access sass files in _bower_components",
      "main": [
        "fonts/*"
      ],
      "normalize": {
        "fonts": [
          "*.otf",
          "*-webfont.*"
        ]
      }
    },
    "isotope": {
      "main": [
        "dist/isotope.pkgd.js"
      ]
    },
    "queryloader2": {
      "main": [
        "jquery.queryloader2.js"
      ]
    },
    "doc-ready": {
      "ignore": true
    },
    "eventEmitter": {
      "ignore": true
    },
    "eventie": {
      "ignore": true
    },
    "fizzy-ui-utils": {
      "ignore": true
    },
    "get-size": {
      "ignore": true
    },
    "get-size-property": {
      "ignore": true
    },
    "masonry": {
      "ignore": true
    },
    "matches-selector": {
      "ignore": true
    },
    "outlayer": {
      "ignore": true
    }
  }
}
