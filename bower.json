{
  "name": "makotokw.com",
  "version": "1.0.0",
  "dependencies": {
    "jquery": "~1.9.0",
    "jquery.easing": "~1.3.0",
    "moment": "~2.15.0",
    "underscore": "~1.8.3",
    "underscore.string": "~2.4.0",
    "backbone": "~1.3.3",
    "modernizr": "~2.8.3",
    "bootstrap-sass": "~3.3.0",
    "font-awesome": "~4.6",
    "isotope": "~2.0.0",
    "bootstrap-material-design": "~0.5.10",
    "jquery.githubRepoWidget": "https://raw.githubusercontent.com/JoelSutherland/GitHub-jQuery-Repo-Widget/master/jquery.githubRepoWidget.js",
    "headroom.js": "./source/_assets/bower_packages/headroom.js.zip"
  },
  "resolutions": {
    "jquery": "1.9.1"
  },
  "overrides_comment": "for main-bower-file: https://github.com/ck86/main-bower-files#overrides-options",
  "overrides": {
    "bootstrap-sass": {
      "main": [
        "assets/fonts/bootstrap/*"
      ],
      "normalize": {
        "fonts": [
          "glyphicons-*"
        ]
      }
    },
    "font-awesome": {
      "comment": "directly access sass files in _bower_components",
      "main": [
        "fonts/*"
      ],
      "normalize": {
        "fonts": [
          "*.otf",
          "*-webfont.*"
        ]
      }
    },
    "github-widget": {
      "main": [
        "dist/widget.js"
      ]
    },
    "queryloader2": {
      "main": [
        "jquery.queryloader2.js"
      ]
    }
  }
}
