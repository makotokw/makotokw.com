{
    "name": "makotokw.com",
    "version": "1.0.0",
    "dependencies": {
        "jquery": "~1.9.0",
        "jquery.easing": "~1.3.0",
        "underscore": "~1.5.0",
        "underscore.string": "~2.3.3",
        "backbone": "~1.1.2",
        "modernizr": "~2.6.2",
        "sass-bootstrap": "~3.0.0",
        "font-awesome": "~4.1",
        "isotope": "~2.0.0",
        "queryloader2": "~2.9.0"
    },
    "exportsOverride": {
        "jquery": {
            "js": [
                "jquery*.js",
                "jquery*.map"
            ]
        },
        "jquery.easing": {
            "js": [
                "js/jquery.easing.js"
            ]
        },
        "jquery-ui": {
            "js": "ui/jquery-ui.js"
        },
        "underscore": {
            "js": [
                "underscore*.js",
                "underscore*.map"
            ]
        },
        "backbone": {
            "js": [
                "backbone*.js"
            ]
        },
        "sass-bootstrap": {
            "js": "dist/js/*.js",
            "sass": "lib/_*.scss",
            "fonts": "dist/fonts/*.*"
        },
        "font-awesome": {
            "sass": "scss/*.scss",
            "fonts": "fonts/*.*"
        },
        "queryloader2": {
            "js": [
                "jquery.queryloader2.js"
            ]
        },
        "isotope": {
            "js": [
                "dist/isotope.pkgd.js"
            ]
        }
    }
}
